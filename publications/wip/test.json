{
"action_class": "dlopt.tools.optimization.Optimizer",
"algorithm_class": "dlopt.architecture.problems.MuPlusLambda",
"data_loader_class": "publications.wip.rubbish.RubbishDataLoader",
"data_loader_params": {"filename": "rubbish-2013.csv",
                       "scaler_class": "sklearn.preprocessing.MinMaxScaler"},
"problem_class": "dlopt.architecture.contrib.TimeSeriesHybridMRSProblem",
"targets": {"log_p": 1},
"population_size": 2,
"offspring_size": 1,
"max_eval": 3,
"max_restart": 1,
"migration_population_size": 1,
"p_mutation_i": 0.25,
"p_mutation_e": 0.25,
"mutation_scale_factor": 3,
"num_samples": 100,
"min_neurons": 10,
"max_neurons": 300,
"min_layers": 1,
"max_layers": 2,
"min_look_back": 2,
"max_look_back": 30,
"x_features": ["C-A100", "C-A107"],
"y_features": ["C-A100", "C-A107"],
"truncated_lower": 0.0,
"truncated_upper": 1.0,
"threshold": 0.01,
"nn_builder_class": "dlopt.nn.RNNBuilder",
"dense_activation": "sigmoid",
"output_logger_class": "dlopt.util.JSONOutput",
"output_logger_params": {"filename": "output.json"},
"model_filename": "model.hdf5",
"training_split": 0.8,
"epochs": 10,
"nn_metric_func": "dlopt.util.mae_loss"
}
